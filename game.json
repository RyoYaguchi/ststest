{
	"width": 1280,
	"height": 720,
	"fps": 30,
	"main": "./script/main.js",
	"assets": {
		"player": {
			"type": "image",
			"width": 32,
			"height": 32,
			"path": "image/player.png"
		},
		"shot": {
			"type": "image",
			"width": 16,
			"height": 16,
			"path": "image/shot.png"
		},
		"se": {
			"type": "audio",
			"path": "audio/se",
			"systemId": "sound",
			"duration": 250,
			"hint": {
				"extensions": [
					".aac",
					".ogg"
				]
			}
		},
		"main": {
			"type": "script",
			"path": "script/main.js",
			"global": true
		},
		"spireData": {
			"type": "script",
			"path": "script/spireData.js",
			"global": true
		},
		"spireRender": {
			"type": "script",
			"path": "script/spireRender.js",
			"global": true
		},
		"spireLogic": {
			"type": "script",
			"path": "script/spireLogic.js",
			"global": true
		},
		"spireUseCard": {
			"type": "script",
			"path": "script/spireUseCard.js",
			"global": true
		},
		"gameInitializer": {
			"type": "script",
			"path": "script/gameInitializer.js",
			"global": true
		},
		"spireEvents": {
			"type": "script",
			"path": "script/spireEvents.js",
			"global": true
		},
		"spireScenes": {
			"type": "script",
			"path": "script/spireScenes.js",
			"global": true
		}
	},
	"environment": {
		"sandbox-runtime": "3"
	}
}